{
  "name": "nextjs-demo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "prod:env": "dotenv -e .env.production.local -e .env.local -e .env",
    "db:migrate": "npm run prod:env -- npx prisma migrate deploy",
    "prisma:studio": "npm run prod:env -- npx prisma studio -- --browser none --",
    "dev:env": "dotenv -e .env.development.local -e .env.local -e .env",
    "dev:db:prisma:studio": "npm run dev:env -- npx prisma studio -- --browser none --",
    "dev:db:seed": "npm run dev:env -- npx tsx ./src/database/utils/dev/seed.ts",
    "dev:db:migrate": "npm run dev:env -- npx prisma migrate dev --",
    "dev:db:generate": "npm run dev:env -- npx prisma generate --",
    "dev:db:reset": "npm run dev:env -- npx prisma migrate reset -- --force --",
    "dev:db:clear:generated": "npm run dev:env -- npx tsx ./src/database/utils/dev/clearGenerated.ts",
    "dev:db:clear:migrations": "npm run dev:env -- npx tsx ./src/database/utils/dev/clearMigrations.ts",
    "test:env": "dotenv -e .env.test.local -e .env",
    "test:build": "npm run test:env -- next build",
    "test:start": "npm run test:env -- next start",
    "test": "npm run test:db:reset -- --force && npm run test:build && npm run test:start",
    "test:db:prisma:studio": "npm run test:env -- npx prisma studio -- --browser none --",
    "test:db:migrate": "npm run test:env -- npx prisma migrate deploy --",
    "test:db:push": "npm run test:env -- npx prisma db push --",
    "test:db:reset": "npm run test:env -- npx prisma migrate reset --",
    "test:db:generate": "npm run test:env -- npx prisma generate --",
    "test:db:seed": "npm run test:env -- npx tsx ./src/database/utils/test/seedExec.ts",
    "test:jest": "npm run test:env -- jest --",
    "test:cypress:open": "npm run test:env -- cypress open --",
    "test:cypress:e2e": "npm run test:env -- cypress run --e2e --"
  },
  "dependencies": {
    "@base-ui-components/react": "^1.0.0-rc.0",
    "@mantine/charts": "^8.3.10",
    "@mantine/core": "^8.3.10",
    "@mantine/dates": "^8.3.10",
    "@mantine/hooks": "^8.3.10",
    "@mantine/notifications": "^8.3.10",
    "@mantine/tiptap": "^8.3.10",
    "@next/env": "^16.0.8",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@tanstack/react-table": "^8.21.3",
    "@tiptap/extension-highlight": "^3.13.0",
    "@tiptap/extension-link": "^3.13.0",
    "@tiptap/extension-subscript": "^3.13.0",
    "@tiptap/extension-superscript": "^3.13.0",
    "@tiptap/extension-task-item": "^3.13.0",
    "@tiptap/extension-task-list": "^3.13.0",
    "@tiptap/extension-text-align": "^3.13.0",
    "@tiptap/pm": "^3.13.0",
    "@tiptap/react": "^3.13.0",
    "@tiptap/starter-kit": "^3.13.0",
    "@vercel/blob": "^2.0.0",
    "bcryptjs": "^3.0.3",
    "better-auth": "^1.4.6",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.19",
    "decode-formdata": "^0.9.0",
    "immer": "^11.0.1",
    "jose": "^6.1.3",
    "next": "^16.0.8",
    "nodemailer": "^7.0.11",
    "pg": "^8.16.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-icons": "^5.5.0",
    "recharts": "^3.5.1",
    "sanitize-html": "^2.17.0",
    "use-immer": "^0.11.0",
    "uuid": "^13.0.0",
    "valibot": "^1.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@ts-safeql/eslint-plugin": "^4.0.8",
    "@types/jest": "^30.0.0",
    "@types/node": "^25",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/sanitize-html": "^2.16.0",
    "baseline-browser-mapping": "^2.9.6",
    "cypress": "^15.8.1",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.8",
    "glob": "^13.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prisma": "^7.1.0",
    "sass": "^1.95.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
