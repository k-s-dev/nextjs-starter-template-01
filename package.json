{
  "name": "nextjs-starter-template-01",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "prod:env": "dotenv -e .env.production.local -e .env.local -e .env",
    "db:migrate": "npm run prod:env -- npx prisma migrate deploy",
    "prisma:studio": "npm run prod:env -- npx prisma studio -- --browser none --",
    "dev:env": "dotenv -e .env.development.local -e .env.local -e .env",
    "dev:db:prisma:studio": "npm run dev:env -- npx prisma studio -- --browser none --",
    "dev:db:seed": "npm run dev:env -- npx tsx ./src/database/utils/dev/seed.ts",
    "dev:db:migrate": "npm run dev:env -- npx prisma migrate dev --",
    "dev:db:generate": "npm run dev:env -- npx prisma generate --",
    "dev:db:reset": "npm run dev:env -- npx prisma migrate reset -- --force --skip-seed --",
    "dev:db:clear:generated": "npm run dev:env -- npx tsx ./src/database/utils/dev/clearGenerated.ts",
    "dev:db:clear:migrations": "npm run dev:env -- npx tsx ./src/database/utils/dev/clearMigrations.ts",
    "test:env": "dotenv -e .env.test.local -e .env",
    "test:build": "npm run test:env -- next build",
    "test:start": "npm run test:env -- next start",
    "test": "npm run test:db:push -- --force-reset --accept-data-loss && npm run test:build && npm run test:start",
    "test:db:prisma:studio": "npm run test:env -- npx prisma studio -- --browser none --",
    "test:db:migrate": "npm run test:env -- npx prisma migrate deploy --",
    "test:db:push": "npm run test:env -- npx prisma db push --",
    "test:db:reset": "npm run test:env -- npx prisma migrate reset --",
    "test:db:generate": "npm run test:env -- npx prisma generate --",
    "test:db:seed": "npm run test:env -- npx tsx ./src/database/utils/test/seedExec.ts",
    "test:jest": "npm run test:env -- jest --",
    "test:cypress:open": "npm run test:env -- cypress open --",
    "test:cypress:e2e": "npm run test:env -- cypress run --e2e --"
  },
  "dependencies": {
    "@base-ui-components/react": "^1.0.0-beta.1",
    "@mantine/core": "^8.3.1",
    "@mantine/dates": "^8.3.1",
    "@mantine/hooks": "^8.3.1",
    "@mantine/notifications": "^8.3.1",
    "@next/env": "^15.4.5",
    "@prisma/adapter-pg": "^6.17.1",
    "@prisma/client": "^6.17.1",
    "@tanstack/react-table": "^8.21.3",
    "@vercel/blob": "^2.0.0",
    "bcryptjs": "^3.0.2",
    "better-auth": "^1.3.27",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.18",
    "decode-formdata": "^0.9.0",
    "immer": "^10.1.3",
    "jose": "^6.0.11",
    "next": "^15.5.4",
    "nodemailer": "^6.10.1",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "use-immer": "^0.11.0",
    "uuid": "^11.1.0",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@ts-safeql/eslint-plugin": "^4.0.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cypress": "^15.5.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "glob": "^11.0.3",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prisma": "^6.17.1",
    "sass": "^1.89.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
